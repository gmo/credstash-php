{
    "name": "gmo/credstash",
    "description": "A utility for managing secrets in the cloud using AWS KMS and DynamoDB",
    "keywords": ["aws", "dynamodb", "kms"],
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Carson Full",
            "email": "carsonfull@gmail.com"
        }
    ],
    "require": {
        "aws/aws-sdk-php": "^3.9",
        "ext-openssl": "*",
        "php": "^5.5.9",
        "symfony/polyfill-php56": "^1.0",
        "webmozart/path-util": "^2.3"
    },
    "require-dev": {
        "symfony/console": "^2.7 || ^3.0",
        "symfony/yaml": "^2.7 || ^3.0"
    },
    "autoload": {
        "psr-4": {
            "CredStash\\": "src"
        }
    },
    "bin": [
        "bin/credstash"
    ],
    "extra": {
        "branch-alias": {
            "dev-master": "1.0-dev"
        }
    }
}
